{
  "lerna": "3.19.0",
  "command": {
    "bootstrap": {
      "npmClient": "yarn",
      "loglevel": "info",
      "registry": "https://registry.npm.taobao.org",
      "ignore": []
    },
    "add": {
      "npmClient": "yarn",
      "loglevel": "info",
      "registry": "https://registry.npm.taobao.org",
      "ignore": []
    },
    "publish": {
      "npmClient": "npm",
      "loglevel": "info",
      "ignoreChanges": [
        "cfx.app-rax",
        "cfx.app-remax"
      ]
    }
  },
  "packages": [
    "packages/cfx.dom",
    "packages/cfx.styl",
    "packages/cfx.builder"
  ],
  "version": "0.2.1"
}
